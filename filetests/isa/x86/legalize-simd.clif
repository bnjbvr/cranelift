test compile
set probestack_enabled=false
target x86_64

; use baldrdash calling convention here for simplicity (avoids prologue, epilogue)
function %test_simd() -> i32x4 [%xmm0] baldrdash {
ebb0:
    v0 = iconst.i32 42
    v1 = splat.i32x4 v0
    ; sameln: v2 = bitcast.i32x4 v0
    ; nextln: v1 = x86_pshuf.i32x4 v2, 0
    return v1
}
